{
	"name": "Nicerly",
	"version": "1.0",
	"description": "An extension that scans messages to make sure they aren't mean.",
	"manifest_version": 2 ,
	"permissions": [
		"https://nicerly.azurewebsites.net/"
	],
	"browser_action": {
		"default_popup": "popup.html"
	} ,
	"content_scripts": [{
		"matches": ["https://www.messenger.com/*"],
		"css": ["speech.css"] ,
		"js": ["click.js"]
	}]
}
